-SET &&SUPERUSER ='Y'; 
-INCLUDE CUSTOMERS.FEX
-*----------------------------------------------------------------FOR read data from database using [READFILE] 
 TABLE FILE SUPERUSER
 PRINT NAME LOCATION AS LOCA
 ON TABLE SET ASNAMES ON
 ON TABLE HOLD AS USER_ADMIN FORMAT ALPHA
 END
