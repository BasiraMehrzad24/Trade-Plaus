-SET &NAME= ' ';
-SET  &LOCA= ' ';

-READFILE USER_ADMIN
-CLOSE USER_ADMIN

-DEFAULT &NAME= ' ';
-DEFAULT  &LOCA= ' ';

-SET  &&NAME= &NAME;
-SET  &&LOCA= &LOCA;

-TYPE &&NAME &&LOCA


